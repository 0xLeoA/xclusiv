pragma solidity ^0.8.0; 



import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/ERC721Enumerable.sol";


contract NounsNFT is  ERC721Enumerable {
    mapping(uint256 => string) tokenIdToImageURL;

    constructor() ERC721("Nouns", "NOUNS") {
        mint(msg.sender, 0, "https://www.google.com/url?sa=i&url=https%3A%2F%2Fluckytrader.com%2Farticles%2Fwhat-is-a-nouns-nft-the-nouns-dao-project-you-should-know-about&psig=AOvVaw35yOI_JxLPoiGXgkdKnByG&ust=1695642234261000&source=images&cd=vfe&opi=89978449&ved=0CAQQjB1qFwoTCLiGhdyVw4EDFQAAAAAdAAAAABAR");
        mint(msg.sender, 1, "https://brave.com/static-assets/images/optimized/nouns-dao/images/image3.png");
        mint(msg.sender, 2, "https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcTbgjhCtGZU8O86omDRrLytwi_7qtZUuGCwjg&usqp=CAU");
        mint(msg.sender, 3, "data:image/png;base64,");
    }

    function mint(address receiver, uint256 tokenId, string memory imageURL)  public {
        _mint(receiver, tokenId);
        tokenIdToImageURL[tokenId] = imageURL;
    }

    function getTokenIdToImageUrl(uint256 tokenId) public view returns(string memory) {
        return tokenIdToImageURL[tokenId];
    }
}
